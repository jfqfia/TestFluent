		setupTokenizerNormalizeSpecActivate 
			//setupTokenizerNormalizeSpecDeactivate 
			//setupTokenizerNormalizeSpecDouble 
		setupTokenizerNormalizeInputActivate
			//setupTokenizerNormalizeInputDeactivate
		setupRuntimeSpyMessageOn
			//TokenizerStrategy:Base
		setupTokenizerSpellingChecker

	classModel
		#Include "Dependencies/MedicalAppointments/MedicalAppointmentParametersTypes.lkt"
		#Include "Kernel/MainTypes.lkt"
