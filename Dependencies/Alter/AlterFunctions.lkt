ScriptNodeStateName validFormat(ParameterType parameterType, string value)
{
	ParameterFormats formats;

	if(!! formats)
	{
		formats <- getFormatsFromParameterTypeKernel(parameterType);
	}

	// Domain: BankingManagement
	if(!! formats)
	{
		formats <- getFormatsFromParameterTypeBankingManagement( parameterType );
	}

	// Domain: MedicalAppointment
	if(!! formats)
	{
		formats <- getFormatsFromParameterTypeMedicalAppointment( parameterType );
	}

	if(!! formats)
	{
		return 'correct';
	}

	return checkFormats(formats, value);
}

procedure checkParameter(string parameterString)
{
	ScriptNode currentNode <- getCurrentNode();
	int infoNumber <- ShapeToInt( parameterString );
	DialogueScript script <- getCurrentScript();
	ScriptInfoItem scriptInfoItem;
	BatchRecoverPosition( script.ScriptInfoItems, infoNumber , scriptInfoItem );
	Parameter parameter <- scriptInfoItem.InfoItemValue.Parameter; 

	ScriptNodeStateName state;
	if(!! state)
	{
		state <- checkParameterKernel(parameter);
	}

	// Domain: BankingManagement
	if(!! state)
	{
		state <- checkParameterBankingManagement( parameter );
	}

	// Domain: MedicalAppointment
	if(!! state)
	{
		state <- checkParameterMedicalAppointment( parameter );
	}

	setNodeState( script.ScriptDescriptor, currentNode.ScriptNodeDescriptor, state );		
}


procedure checkParameterIntersentential(string parameterString)
{
	ScriptNode currentNode <- getCurrentNode();
	int infoNumber <- ShapeToInt( parameterString );
	DialogueScript script <- getCurrentScript();
	ScriptInfoItem scriptInfoItem;
	BatchRecoverPosition( script.ScriptInfoItems, infoNumber , scriptInfoItem );
	Parameter parameter <- scriptInfoItem.InfoItemValue.Parameter; 

	ScriptNodeStateName state;
	if(!! state)
	{
		state <- checkParameterIntersententialKernel(scriptInfoItem.InfoItemHistory, parameter);
	}

	// Domain: BankingManagement
	if(!! state)
	{
		state <- checkParameterIntersententialBankingManagement(scriptInfoItem.InfoItemHistory, parameter );
	}

	// Domain: MedicalAppointment
	if(!! state)
	{
		state <- checkParameterIntersententialMedicalAppointment(scriptInfoItem.InfoItemHistory, parameter );
	}
	
	setNodeState( script.ScriptDescriptor, currentNode.ScriptNodeDescriptor, state );		
}

