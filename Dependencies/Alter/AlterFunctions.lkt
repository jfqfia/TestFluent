ParameterValue concatenateHistory(InfoItemHistory history, Parameter lastParameter)
{
	ParameterValue ret;

	if(!! ret)
	{
		ret <- concatenateHistoryKernel(history, lastParameter);
	}

	// Domain: BankingManagement
	if(!! ret)
	{
		ret <- concatenateHistoryBankingManagement(history, lastParameter);
	}

	// Domain: MedicalAppointment
	if(!! ret)
	{
		ret <- concatenateHistoryMedicalAppointment(history, lastParameter);
	}

	return ret;
}


