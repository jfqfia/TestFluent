		}
		case 'peselnumber' 
		{
			ret <- canMergeWithSequentialPeselNumber(parameter1, parameter2);
		}
		case 'accountnumber' 
		{
			ret <- canMergeWithSequentialAccountNumber(parameter1, parameter2);
		}
		case 'moneyamount' 
