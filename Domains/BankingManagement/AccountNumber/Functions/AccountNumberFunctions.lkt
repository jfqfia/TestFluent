bool checkAccountNumberValidity( Parameter parameter )
{
	string literal <- parameter.ParameterValue.AccountNumber.AccountNumberValue;

	if( LiteralSize(literal) == 10 )
	{
		return True;
	}

	return False;
}

AccountNumber convertNumberToAccountNumber(Parameter parameter)
{
	AccountNumber ret;

	if(parameter.ParameterCategory == 'terminal')
	{
		NumberValue number <- parameter.ParameterValue.Number.NumberValue;
		ret.AccountNumberValue <- ShapeToString(number);
	}
	if(parameter.ParameterCategory == 'sequential')
	{
		ret.AccountNumberValue <- getConcatenatedNumber( parameter );
	}

	return ret;
}

string getConcatenatedNumber( Parameter parameter )
{
	if( parameter.ParameterCategory == 'terminal' )
	{
		return ShapeToString(parameter.ParameterValue.Number.NumberValue);
	}
	else
	{
		string ret;

		ret <- getConcatenatedNumber( parameter.ParameterOperand1 );
		ret <- LiteralConcat( ret, getConcatenatedNumber ( parameter.ParameterOperand2 ) );
		return ret;
	}
}

