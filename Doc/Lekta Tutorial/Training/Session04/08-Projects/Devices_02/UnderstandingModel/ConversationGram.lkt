(RConversation2 : 
	[Conversation/0 -> lexAgree]
		{ ^ <- (Agree:'on'); } )

(RConversation3 : 
	[Conversation/0 -> lexRefuse]
		{ ^ <- (Refuse:'on'); } )

(RConversation4 : 
	[Conversation/0 -> lexRepeat]
		{ ^ <- (Repeat:'on'); } )

(RConversation5 : 
	[Conversation/2 -> Conversation/2 lexConversation]
		{ ^ <- #1; } )

(RConversation6 : 
	[Conversation/3 -> lexConversation Conversation/3]
		{ ^ <- #2; } )

(RConversation7 : 
	[Conversation/3 -> Conversation/3 Conversation/2]
		{ ^ <- #2; } )
