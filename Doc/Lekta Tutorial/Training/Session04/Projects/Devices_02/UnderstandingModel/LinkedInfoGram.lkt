(RLinkedInfo1 : 
	[ LinkedInfo -> Location ] 
		{ ^.LinkedInfoDescriptor <- 'location';
		  ^.Location <- #1; } )

(RLinkedInfo1 : 
	[ LinkedInfo -> Temperature ] 
		{ ^.LinkedInfoDescriptor <- 'temperature';
		  ^.Temperature <- #1; } )

