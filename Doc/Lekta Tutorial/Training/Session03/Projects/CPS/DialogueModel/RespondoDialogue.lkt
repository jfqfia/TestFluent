	BatchExtractInit($MINDBOARD@OStruct.OScheme,loutput);
 
	//SpyMessage("Respondo: ",loutput.LMode,"   ",loutput.TField);
        ^OBJRESPONDO <- loutput;

	RespondoRetry();
    }
)
