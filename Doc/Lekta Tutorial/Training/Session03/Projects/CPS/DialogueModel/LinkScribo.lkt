/*******************************
 ** LinkScribo
 *******************************/

integer	LinkScribo(TaskLine tline)
{
	integer age;

	if (tline.WData.PatientData) {
		age <- ClockAskYear() - tline.WData.PatientData.PatientBirthGlobalDate.BirthGlobalDatePeriodYear;
		//SpyMessage("LinkScribo: ",age);
		LinkScriboUserProfile(age);
	}

	return 1;
}

integer LinkScriboUserProfile(integer age)
{
	//	SpyMessage("LinkScriboUserProfile: ",age);

	SetContextLinkIntegerBetween(1,age);

	return 1;
}
