(SensoScheme Booting : [ControlModel]
	SensoCapture 
	{
        	#OBJSENSO-1.Position == 'start'
	}
	SensoAction 
	{
		$MINDBOARD@Control.Position <- #OBJSENSO-1.Position;
	}
)

(SensoScheme R1 : [ UserProference ]
	SensoAction 
	{
        	BatchInsertEnd($MINDBOARD@Input.UserProferences, #OBJSENSO-1);
        }
)

