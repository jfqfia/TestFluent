lektaProject
	projectHead
		projectDescriptor	: "Fluency"
		projectLanguageScope	: [ English, Spanish, Polish ]
		projectOperationScope	: [ Fluency ]
		projectInterfaceScope   : [ Written, Spoken ]
		projectCompileOutput 	: "Deployment/bin/AlterFluency.olk"

	projectSetup
		setupParserRoots = ProferenceChunk
		setupTokenizerNormalizeSpecActivate 
			//setupTokenizerNormalizeSpecDeactivate 
			//setupTokenizerNormalizeSpecDouble 
		setupTokenizerNormalizeInputActivate
			//setupTokenizerNormalizeInputDeactivate
		setupRuntimeSpyMessageOn
			//TokenizerStrategy:Base
		setupTokenizerSpellingChecker

	classModel
		#Include "Dependencies/Alter/AlterParametersTypes.lkt"
		#Include "Kernel/MainTypes.lkt"
		#Include "Domains/MedicalAppointments/MainMedicalAppointmentTypes.lkt"
		#Include "Domains/BankingManagement/MainBankingManagementTypes.lkt"

	functionModel
		#Include "Dependencies/Alter/AlterFunctions.lkt"
		#Include "Kernel/MainFunctions.lkt"
		#Include "Domains/MedicalAppointments/MainMedicalAppointmentFunctions.lkt"
		#Include "Domains/BankingManagement/MainBankingManagementFunctions.lkt"

		#Include "Kernel/DialogueManager/Functions/DialogueScheme.lkth"
		#Include "Kernel/DialogueManager/Functions/DialogueScript.lkth"

	lexicalModel forLanguage English 
		#Include "Kernel/MainEnglishLexicon.lkt"
		#Include "Domains/MedicalAppointments/MainMedicalAppointmentEnglishLexicon.lkt"
		#Include "Domains/BankingManagement/MainBankingManagementEnglishLexicon.lkt"

	grammaticalModel forLanguage English
		#Include "Kernel/MainEnglishGrammar.lkt"
		#Include "Domains/MedicalAppointments/MainMedicalAppointmentEnglishGrammar.lkt"
		#Include "Domains/BankingManagement/MainBankingManagementEnglishGrammar.lkt"

	conversationalModel
		#Include "Kernel/DialogueManager/MindBoard/MindBoardStructure.lkt"

	LaunchConversation: 
	{
		LaunchSendToMindBoard: (DialogueState = (DialoguePhase: 'booting') )
	}

	ColligoSchemata
		#Include "Kernel/DialogueManager/Colligo/ColligoSchemata.lkt"

	SensoSchemata
		#Include "Kernel/DialogueManager/Senso/SensoSchemata.lkt"

	CogitoSchemata
		#Include "Kernel/DialogueManager/Cogito/CogitoSchemata.lkt"

	functionModel
		#Include "Kernel/DialogueManager/Functions/DialogueScheme.lkt"
		#Include "Kernel/DialogueManager/Functions/DialogueScript.lkt"

